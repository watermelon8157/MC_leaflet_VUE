                    <tr>
                        <td colspan="9" class="ALLBorder">
                            Diagnosis：
                            @if (!string.IsNullOrWhiteSpace(patientInfo.diagnosis_code) && patientInfo.diagnosis_code.Length >= 50)
                            {
                                @patientInfo.diagnosis_code.Substring(0, 50)
                                @:...
                            }
                            else
                            {
                                @patientInfo.diagnosis_code
                            }
