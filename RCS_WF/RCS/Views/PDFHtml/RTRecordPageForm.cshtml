                                    pList.Add(string.Format("hfov hz:{0}", o.rt_record.hfov_hz));
                                }
                                @if (!string.IsNullOrWhiteSpace(o.rt_record.hfov_mean))
                                {
                                    pList.Add(string.Format("hfov mean:{0}", o.rt_record.hfov_mean));
                                }
                                @Html.Raw(string.Join(", ", pList))
