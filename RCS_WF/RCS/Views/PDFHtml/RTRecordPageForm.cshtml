                                        @Html.Raw(string.Format("{0}/{1}", o.rt_record.mode, o.rt_record.device_o2))
                                    }
                                    else
                                    {
                                        @Html.Raw(string.Format("{0}/{1}", o.rt_record.mode, o.rt_record.device_o2))
                                    }
                                }
                                else
