                        </td>
                        @foreach (RT_RECORD_MAIN o in item)
                        {
                            <td class="BRBorder rowDataStyle" align="center" valign="middle" style="@(rowDataStyleFont)">
                                @if (!string.IsNullOrWhiteSpace(o.RECORDDATE))
                                {
                                    @o.showRECORDDATE.ToString("yyy-MM-dd")
                                }
                                @if (!string.IsNullOrWhiteSpace(o.RECORDDATE))
                                {
                                    @o.showRECORDDATE.ToString("HH:mm")
                                }
                            </td>
                        }
