@model RCS.Models.ViewModel.ListprintRtCptReq
@using RCS_Data;
@{


    string DeviantImage = Com.Mayaminer.IniFile.GetConfig("System", "DeviantImage");
    DeviantImage = "data:image/png;base64,";
}

@for (int y = 0; y < Model.List.Count(); y++)
{
    UserInfo user_info = Model.List[y].user_info;
    IPDPatientInfo pat_info = Model.List[y].pat_info;

    <table style="width:100%;height:100%;font-size:14px;" cellspacing="0" cellpadding="3">
        <tr>
            <td align="center" valign="middle" class="tilteSize tilteFontBold" style="font-size:24px;" colspan="9">呼吸治療申請單 </td>
            <td colspan="3"  >
                <table style="font-size:12px;width:100%;" cellspacing="0" cellpadding="3" class="ALLBorder">
                    <tr>
                        <td>病歷號:</td>
                        <td>@(Model.List[y].data.record_id == null ? "" : pat_info.chart_no)</td>
                        <td>床號:</td>
                        <td>@(Model.List[y].data.record_id == null ? "" : pat_info.bed_no)</td>
                    </tr>
                    <tr>
                        <td>姓名:</td>
                        <td>@(Model.List[y].data.record_id == null ? "" : pat_info.patient_name)</td>
                        <td>性別:</td>
                        <td>@(Model.List[y].data.record_id == null ? "" : pat_info.genderCHT)</td>

                    </tr>
                    <tr>
                        <td>出生日期:</td>
                        <td>
                            @(Model.List[y].data.record_id == null ? "" : pat_info.birth_day_CHT) @(Model.List[y].data.record_id == null ? "" : "(" + pat_info.age + ")")
                        </td>
                        <td>診別:</td>
                        <td>@(Model.List[y].data.record_id == null ? "" : pat_info.nam_c)  </td>
                    </tr>
                </table>
            </td>
        </tr>
        <tr>
            <td colspan="9" style="font-size:20px;width:100%;">
                以下資料請務必填寫，謝謝您的配合。
            </td>
            <td colspan="4" >
               
            </td>
        </tr>
        <tr>
            <td colspan="11" style="font-size:14px;width:100%;">
                診斷： <u>@(Model.List[y].data.record_id == null ? "________________________________________________________" : Model.List[y].data.now_diagnosis_display)</u>

            </td>
           
        </tr>
        <tr>
            <td colspan="11" style="font-size:14px;width:100%;">
                主治醫師：<u>@(Model.List[y].data.record_id == null ? "________________________" : Model.List[y].data.vs_doc)</u> 填單醫師：<u>@(Model.List[y].data.record_id == null ? "________________________" : Model.List[y].data.set_doc)</u>
            </td>
        </tr>
        <tr>
            <td colspan="11" style="font-size:14px;width:100%;">
                身高：<u>@(Model.List[y].data.record_id == null ? "_________" : Model.List[y].data.height)</u>cm 體重：<u>@(Model.List[y].data.record_id == null ? "_________" : Model.List[y].data.weight)</u>kg 體重：<u>@(Model.List[y].data.record_id == null ? "_________" : Model.List[y].data.gcs)</u>
            </td>
        </tr>
        <tr>
            <td colspan="11" style="font-size:14px;width:100%;">
                過去病史：
                @(Model.List[y].data.record_id == null || Model.List[y].data.reconditioning_exercise == null ? "□" : Model.List[y].data.reconditioning_exercise.Contains("Asthma") ? "■" : "□") Asthma
                @(Model.List[y].data.record_id == null || Model.List[y].data.reconditioning_exercise == null ? "□" : Model.List[y].data.reconditioning_exercise.Contains("COPD") ? "■" : "□") COPD
                @(Model.List[y].data.record_id == null || Model.List[y].data.reconditioning_exercise == null ? "□" : Model.List[y].data.reconditioning_exercise.Contains("TB/Old TB") ? "■" : "□") TB/Old TB
                @(Model.List[y].data.record_id == null || Model.List[y].data.reconditioning_exercise == null ? "□" : Model.List[y].data.reconditioning_exercise.Contains("Lung Cancer") ? "■" : "□") Lung Cancer
                @(Model.List[y].data.record_id == null || Model.List[y].data.reconditioning_exercise == null ? "□" : Model.List[y].data.reconditioning_exercise.Contains("Pneumoconiosis") ? "■" : "□") Pneumoconiosis
                <br />@(Model.List[y].data.record_id == null || Model.List[y].data.reconditioning_exercise == null ? "□" : Model.List[y].data.reconditioning_exercise.Contains("ARDS") ? "■" : "□") ARDS
                @(Model.List[y].data.record_id == null || Model.List[y].data.reconditioning_exercise == null ? "□" : Model.List[y].data.reconditioning_exercise.Contains("Other") ? "■" : "□") Other
                @(Model.List[y].data.record_id == null ? "_________" : Model.List[y].data.Past_medical_history_other)
            </td>
        </tr>
        <tr>
            <td colspan="11" style="font-size:14px;width:100%;">
                填單日期：<u>@(Model.List[y].data.record_id == null ? "__________________________" : Model.List[y].data.rec_date)</u>
            </td>
        </tr>
        <tr>
            <td colspan="11" style="font-size:14px;width:100%;">
                最近一次 chest X-ray：<u>@(Model.List[y].data.record_id == null ? "__________________________" : Model.List[y].data.X_Ray_date)</u>
            </td>
        </tr>
        <tr>
            <td colspan="12" style="font-size:14px;width:100%;">
                申請項目：
            </td>
        </tr>
        <tr>
            <td colspan="3" style="font-size:14px;" align="center" class="LRBorder topBorder bottomBorder">
                治療項目
            </td>
            <td colspan="9" style="font-size:14px;" align="center" class="BRBorder topBorder">
                請填寫並協助開立醫囑
            </td>
            <td colspan="1" rowspan="5" style="font-size:10px;vertical-align:text-top;">
                @*一<br />式<br />一<br />聯<br />
                :<br />
                申<br />請<br />單<br />位<br />↓<br />呼<br />吸<br />
                治<br />療<br />室<br />
                存*@

            </td>
        </tr>
        <tr>
            <td colspan="3" align="center" class="LBRBorder" >
                Bronchial<br />
                Hygiene<br />
                Therapy
            </td>
            <td colspan="9" class="BRBorder">
                @(Model.List[y].data.record_id == null || Model.List[y].data.reconditioning_exercise == null ? "□" : Model.List[y].data.cpt_check == "true" ? "■" : "□") CPT (Percussion)<br />
                @(Model.List[y].data.record_id == null || Model.List[y].data.reconditioning_exercise == null ? "□" : Model.List[y].data.Percussor_check == "true" ? "■" : "□") Percussor (拍痰器)
                &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;需加強部位：<u>@(Model.List[y].data.record_id == null ? "_____________________" : Model.List[y].data.cpt_strengthen_part)</u><br />
                @(Model.List[y].data.record_id == null || Model.List[y].data.reconditioning_exercise == null ? "□" : Model.List[y].data.HFCWO_check == "true" ? "■" : "□") HFCWO (VEST)
                &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;禁治療部位：<u>@(Model.List[y].data.record_id == null ? "________________" : Model.List[y].data.cpt_forbidden_treatment)</u>
                <br />
                @(Model.List[y].data.record_id == null || Model.List[y].data.reconditioning_exercise == null ? "□" : Model.List[y].data.Other_check == "true" ? "■" : "□") Other：
                @(Model.List[y].data.record_id == null ? "________" : Model.List[y].data.Bronchial_otrher)
                <br />
                醫囑：
                Consult RT for <u>@(Model.List[y].data.record_id == null ? "____" : Model.List[y].data.cpt)</u> with monitoring SpO<sup>2</sup>
                date: <u>@(Model.List[y].data.record_id == null ? "____" : Model.List[y].data.cpt_days)</u>  <u>@(Model.List[y].data.record_id == null ? "____" : Model.List[y].data.cpt_days_2)</u>(Maximum 7days)
            </td> 
        </tr>
        <tr>
            <td colspan="3" align="center" class="LBRBorder">
                Lung<br />
                Expansion<br />
                Therapy
            </td>
            <td colspan="9" class="BRBorder">
                @(Model.List[y].data.record_id == null || Model.List[y].data.ippb_check == null ? "□" : Model.List[y].data.cpt_check == "true" ? "■" : "□") IPPB&nbsp;&nbsp;
                意識清楚可配合指令 ?
                @(Model.List[y].data.record_id == null || Model.List[y].data.ippb_conscious == null ? "□" : Model.List[y].data.ippb_conscious.Contains("是") ? "■" : "□")是
                @(Model.List[y].data.record_id == null || Model.List[y].data.ippb_conscious == null ? "□" : Model.List[y].data.ippb_conscious.Contains("否") ? "■" : "□")否<br />
                醫囑：Inhalation @(Model.List[y].data.record_id == null ? "____" : Model.List[y].data.ippb_inhalation)
                with IPPB Q<u>@(Model.List[y].data.record_id == null ? "____" : Model.List[y].data.ippb_hours)</u> H
                date:<u>@(Model.List[y].data.record_id == null ? "____" : Model.List[y].data.ippb_days)</u>
                <u>@(Model.List[y].data.record_id == null ? "____" : Model.List[y].data.ippb_days_2)</u> with monitoring SpO<sup>2</sup> (Maximum 7days)<br />
                @(Model.List[y].data.record_id == null || Model.List[y].data.ippb_check == null ? "□" : Model.List[y].data.IS == "true" ? "■" : "□") IS = Triflow (限胸腔大手術後患者申報)&nbsp;&nbsp;
                手術日期:<u>@(Model.List[y].data.record_id == null ? "____" : Model.List[y].data.surgery_date)</u><br />
                醫囑：Consult RT for IS/Triflow Training with monitoring SpO<sub>2</sub><br />


                @(Model.List[y].data.record_id == null || Model.List[y].data.ippb_check == null ? "□" : Model.List[y].data.Negative_Pressure_Ventilation == "true" ? "■" : "□") Negative Pressure Ventilation (負壓呼吸器)&nbsp;&nbsp;
                <br />
                醫囑：Consult RT for NPV using @(Model.List[y].data.record_id == null ? "____" : Model.List[y].data.Negative_inhalation)
                with Q<u>@(Model.List[y].data.record_id == null ? "____" : Model.List[y].data.Negative_hours)</u> H
                date:<u>@(Model.List[y].data.record_id == null ? "____" : Model.List[y].data.Negative_days)</u>
                <u>@(Model.List[y].data.record_id == null ? "____" : Model.List[y].data.Negative_days_2)</u><br />
                &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;with monitoring SpO<sup>2</sup> (Maximum 7days)<br />
                @(Model.List[y].data.record_id == null || Model.List[y].data.ippb_check == null ? "□" : Model.List[y].data.Cough_Machine == "true" ? "■" : "□") Mechanical Insufflation-Exsufflation = Cough Machine(咳痰機)&nbsp;&nbsp;
                <br />
                醫囑：Consult RT for MI-E using @(Model.List[y].data.record_id == null ? "____" : Model.List[y].data.Cough_inhalation)
                with Q<u>@(Model.List[y].data.record_id == null ? "____" : Model.List[y].data.Cough_hours)</u> H
                date:<u>@(Model.List[y].data.record_id == null ? "____" : Model.List[y].data.Cough_days)</u>
                <u>@(Model.List[y].data.record_id == null ? "____" : Model.List[y].data.Cough_days_2)</u><br/>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; with monitoring SpO<sup>2</sup> (Maximum 7days)<br />
            </td> 
        </tr>
        <tr>
            <td colspan="3" align="center" class="LBRBorder">
                Reconditioning<br />
                Exercise
            </td>
            <td colspan="9" class="BRBorder">
                @(Model.List[y].data.record_id == null || Model.List[y].data.reconditioning_exercise == null ? "□" : Model.List[y].data.reconditioning_exercise.Contains("上肢運動") ? "■" : "□") 上肢運動<br />
                @(Model.List[y].data.record_id == null || Model.List[y].data.reconditioning_exercise == null ? "□" : Model.List[y].data.reconditioning_exercise.Contains("下肢運動") ? "■" : "□") 下肢運動&nbsp;&nbsp;&nbsp;&nbsp;
                意識清楚可配合指令 ?
                @(Model.List[y].data.record_id == null || Model.List[y].data.reconditioning_string == null ? "□" : Model.List[y].data.reconditioning_string.Contains("是") ? "■" : "□")是
                @(Model.List[y].data.record_id == null || Model.List[y].data.reconditioning_string == null ? "□" : Model.List[y].data.reconditioning_string.Contains("否") ? "■" : "□")否
                <br />
                @(Model.List[y].data.record_id == null || Model.List[y].data.reconditioning_exercise == null ? "□" : Model.List[y].data.reconditioning_exercise.Contains("軀幹運動(如抬臀、擴胸運動)") ? "■" : "□")	軀幹運動(如抬臀、擴胸運動)<br />
                @(Model.List[y].data.record_id == null || Model.List[y].data.reconditioning_exercise == null ? "□" : Model.List[y].data.reconditioning_exercise.Contains("姿勢擺位訓練(如床頭抬高、練習坐立等)") ? "■" : "□")	姿勢擺位訓練(如床頭抬高、練習坐立等)<br />
                <br />
                醫囑：Consult RT for Reconditioning Exercise + Breathing Exercise date：
                <u>@(Model.List[y].data.record_id == null ? "____" : Model.List[y].data.reconditioning_exercise_days)</u>
                <u>@(Model.List[y].data.record_id == null ? "____" : Model.List[y].data.reconditioning_exercise_days_2)</u>
                <br />&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;with monitoring SpO<sub>2</sub> (Maximum 7days)
                <br />
                @(Model.List[y].data.record_id == null || Model.List[y].data.ippb_check == null ? "□" : Model.List[y].data.Sand_Bag_Training == "true" ? "■" : "□") Sand-Bag Training (沙袋訓練)&nbsp;&nbsp;
                <br />
                醫囑：Consult RT for Sand-Bag Training @(Model.List[y].data.record_id == null ? "____" : Model.List[y].data.Sand_Bag_Training_inhalation)
                Q<u>@(Model.List[y].data.record_id == null ? "____" : Model.List[y].data.Sand_Bag_Training_hours)</u> H (Reconditioning exercise +
                <br />&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;Breathing exercise) with monitoring SpO<sub>2</sub>
                <br />
                @(Model.List[y].data.record_id == null || Model.List[y].data.ippb_check == null ? "□" : Model.List[y].data.SV_Training == "true" ? "■" : "□") Speaking Valve Training (發聲閥訓練)&nbsp;&nbsp;
                意識清楚可配合指令 ? @(Model.List[y].data.record_id == null || Model.List[y].data.SV_Training_inhalation == null ? "□" : Model.List[y].data.SV_Training_inhalation.Contains("是") ? "■" : "□")是
                @(Model.List[y].data.record_id == null || Model.List[y].data.SV_Training_inhalation == null ? "□" : Model.List[y].data.SV_Training_inhalation.Contains("否") ? "■" : "□")否
                <br />
                醫囑：Consult RT for Speaking Valve Training (Reconditioning exercise +
                <br />&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;Breathing exercise) with monitoring SpO<sub>2</sub>
            </td> 
        </tr>
        <tr>

            <td colspan="3" align="center" class="LBRBorder" style="height:50px;">
                Other
            </td>
            <td colspan="9" class="BRBorder">
                @(Model.List[y].data.record_id == null ? "" : Model.List[y].data.Other)
            </td> 
        </tr>
        <tr>
            <td colspan="12" style="font-size:16px;width:100%;">
                收案呼吸治療師回覆<u>@(Model.List[y].data.record_id == null ? "_____________________" : Model.List[y].data.Remark)</u>
            </td>
        </tr>
        <tr>
            <td colspan="6" style="font-size:16px;width:100%;">
                *RT 收案日期<u>@(Model.List[y].data.record_id == null ? "_____________________" : Model.List[y].data.String_date)</u>
            </td>
            <td colspan="6" style="font-size:16px;width:100%;">
                結案日<u>@(Model.List[y].data.record_id == null ? "_____________________" : Model.List[y].data.End_date)</u>
            </td>
        </tr>
    </table>
    <table style="width:97%;">
        <tr>
            <td align="left" style="width:45%;"></td>
            <td align="center" style="width:10%;font-size:10px;">
                第 @(Model.List[y].data.record_id == null ? "___" : "1") 張
            </td>
            <td align="right" style="width:45%;"></td>
        </tr>
        <tr>
            <td align="left" style="float:left;font-size:10px;"> 
            </td>
            <td align="center" style="width:10%;font-size:10px;"></td>
            <td align="right" style="float:right;font-size:10px;">
                 
            </td>
        </tr>
    </table>
    <div>我是分頁</div>
}


