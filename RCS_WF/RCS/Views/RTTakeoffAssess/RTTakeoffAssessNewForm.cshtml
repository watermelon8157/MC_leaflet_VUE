    //儲存
    function tk_Form_save(pThis) {
        var obj = new Object();

        obj.tk_id = $('#tk_form #tk_id').val();
        obj.rec_date = $('#tk_form #rec_date').val();
        obj.DTL = getJsonObject('tk_form');
        obj.DTL.weaningTable = $('#tk_form #weaningTable').bootstrapTable('getData');
        obj.DTL.labDataTable = convert_flat_all($('#tk_form #labDataTable').bootstrapTable('getData'));
        $.ajax({
            url: "@Url.Content("~/RTTakeoffAssess/RTTakeoffNewSave")",
            type: "post",
            data: { rowStr: encodeURIComponent(JSON.stringify(obj)) },
            dataType: "json",
            success: function (data) {
                if (data.status == 0) {
                    //表單重置
                    set_tk_form_View();
                    $("#tk_Form_panel").modal("hide");
                    $("#tk_table").bootstrapTable('refresh');
                }
                showAlert(data.message, msg_stat.Success, 3000);
            },
            beforeSend: function () {
                $(pThis).button('loading');
            },
            complete: function () {
                $(pThis).button('reset');
            },
            error: function () {
                showAlert("存檔錯誤", msg_stat.Danger);
                $(pThis).button('reset');
            }
        });
    }
    //刪除
    function tk_Form_Del(pThis) {
        if (confirm('確認是否要刪除?')) {
            $.ajax({
                url: "@Url.Content("~/RTTakeoffAssess/RTTakeoffNewDel")",
