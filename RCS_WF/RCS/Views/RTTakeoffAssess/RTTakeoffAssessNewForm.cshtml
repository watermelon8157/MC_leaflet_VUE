            $(".setScrollbar").unbind("hidden.bs.modal").on("hidden.bs.modal", function (e) {
                setTimeout(function () { $("#mybody").addClass("modal-open"); }, 0);
            });

 
            //設定停止呼吸器脫離的原因規則 START
            var $gBlock = $('#tk_form #A2_block').find('.gItem');
            var gi_anyone = $('#tk_form #A2_block').find('.form-SBT[id="fs"]').find('.form-item[id="weaning_st_reason_11"]');
            var gii_anytwo = $('#tk_form #A2_block').find('.form-SBT[id="fs"]').find('.form-item[id="weaning_st_reason_12"]');
            var gii_giii = $('#tk_form #A2_block').find('.form-SBT[id="fs"]').find('.form-item[id="weaning_st_reason_13"]');
            $gBlock.each(function () {
                var chk = $(this).find('.form-item');
                chk.each(function () {
                    $(this).click(function () {
                        var giCnt = 0;
                        var giiCnt = 0;
                        var giiiCnt = 0;
                        $gBlock.each(function () {
                            var id = $(this).attr('id');
                            switch (id) {
                                case 'gi':
                                    giCnt = $(this).find("input:checked").length;
                                    break;
                                case 'gii':
                                    giiCnt = $(this).find("input:checked").length;
                                    break;
                                case 'giii':
                                    giiiCnt = $(this).find("input:checked").length;
                                    break;
                                default:
                                    break;
                            }
