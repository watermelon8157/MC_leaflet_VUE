                    $('#nbp_5').val(!!row['rt_record.vr'] ? row['rt_record.vr'] : '');
                    $('#nbp_6').val(!!row['rt_record.pressure_ps'] ? row['rt_record.pressure_ps'] : '');
                    $('#nbp_7').val(!!row['rt_record.pressure_peep'] ? row['rt_record.pressure_peep'] : '');
                    //$('#nbp_9').val('');
                    $('#rtRecord_select_modal').modal('hide');
                });

                $('#weaning_select_tb').bootstrapTable({
                    url: "@Url.Content("~/RTTakeoffAssess/Getweaning_select_tb")",
                    method: "post", search: true, clickToSelect: true 
                });

            //縮放
            $('#tk_Form_panel .panel_head').on('click', function () {
                var _the = $(this);
                $(this).closest('.panel').find('.panel-body').slideToggle('slow', function () {
                    if (_the.hasClass('fa-chevron-circle-down')) {
                        _the.removeClass('fa-chevron-circle-down');
                        _the.addClass('fa-chevron-circle-right');
                    } else {
                        _the.removeClass('fa-chevron-circle-right');
                        _the.addClass('fa-chevron-circle-down');
